    "@types/webpack-env": "^1.16.0",
    "autoprefixer": "^10.2.4",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.8",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
